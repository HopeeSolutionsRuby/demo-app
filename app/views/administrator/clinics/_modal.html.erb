<div class="modal fade" id="editModal<%=clinic.id%>" tabindex="-1" aria-labelledby="addModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Upload Picture</h5>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>

      <div data-controller="dropzone" data-dropzone-target="previewContainer" class="preview-container mt-3 border p-3">
        <div class="input-group">
      </div>
        <%= form.file_field :pictures, id: "clinic_pictures", multiple: true, class: "form-control", "data-dropzone-target": "input" %>
      </div>  
      <label for="clinic_pictures" class=" drag mt-2 mx-3">
        Drag and drop files here or click to upload
      </label>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>