<tr>
  <td><%= jobcategory.country %></td>
  <td><%= jobcategory.job_category_status%></td>
  <td><%= jobcategory.category_name%></td>
  <td><%= jobcategory.description %></td>
  <td>
      <div class="d-grid gap-4 col-5">
              <%= link_to 'Edit', edit_admins_jobcategory_path(jobcategory), class: 'btn btn-primary', data: { turbo_frame: 'container-frame', turbo_action: 'replace' } %>
          </div>
          <div class="d-grid gap-4 col-5" >
            <%= link_to 'Show', admins_jobcategory_path(jobcategory), class: 'btn btn-success', data: { turbo_frame: 'container-frame', turbo_action: 'replace' } %>
          </div>
          <div class="d-grid gap-4 col-6" >
            <%= button_to 'Delete', admins_jobcategory_path(jobcategory), method: :delete, class: 'btn btn-danger', remote: true %>
          </div>

  </td>
</tr>
